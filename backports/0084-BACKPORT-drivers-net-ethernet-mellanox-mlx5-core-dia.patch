 	strscpy_pad(trace_data->msg, msg, TRACE_STR_MSG);
+#else
+	strncpy(trace_data->msg, msg, TRACE_STR_MSG);
+#endif
 
 	tracer->st_arr.saved_traces_index =
 		(tracer->st_arr.saved_traces_index + 1) & (SAVED_TRACES_NUM - 1);
