 			mlx5_tracer_clean_message(str_frmt);
 	}
 }
@@ -548,7 +552,11 @@ static void mlx5_fw_tracer_save_trace(st
 	trace_data->timestamp = timestamp;
 	trace_data->lost = lost;
 	trace_data->event_id = event_id;
